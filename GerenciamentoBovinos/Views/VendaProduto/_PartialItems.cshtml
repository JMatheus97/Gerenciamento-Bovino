@*@model IEnumerable<GerenciamentoBovinos.Models.ItemsVendaProduto>*@

<table class="table">
    <tr>
        @*<th>
                @Html.DisplayName("Teste")
            </th>*@
        <th>
            @Html.DisplayName("Quantidade")
        </th>
