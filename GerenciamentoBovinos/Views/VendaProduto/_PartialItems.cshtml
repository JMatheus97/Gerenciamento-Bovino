                @Html.DisplayName(model => model.ValorUnitario)
            </th>
            <th>
                @Html.DisplayName(model => model.ValorTotal)
            </th>*@
    </tr>

    @foreach (var item in (List<GerenciamentoBovinos.Models.ItemsVendaProduto>)ViewBag.Items)
